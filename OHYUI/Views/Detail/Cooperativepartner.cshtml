@model List<CooperativePartner>
@{
    ViewBag.Title = UIHelper.Word("合作伙伴");
}

@section header{
    <script type="text/javascript" src="~/Scripts/jquery-lbt/js/script.js"></script>
    <link href="~/Content/cooperativepartner.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-lbt/css/css.css" rel="stylesheet" type="text/css" />
    <style>
        #box ul li {
            border-color: #878c92;
        }
    </style>
}

<div class="coo-bg">
    <div class="title-logo">
        <img src="/images/coologo.png" alt="">
    </div>

    <div id="box">
        <pre class="prev">prev</pre>
        <pre class="next">next</pre>
        <ul>
            @for (int i = 0; i < 2; i++)
            {
                foreach (var item in Model)
                {
                    <li>
                        <img src="@item.ImgPath" />
                    </li>
                }
            }
        </ul>
    </div>
</div>