@using OHYModel; 
@using MongoDB.Bson;
@{
    UM userlogin = null;
    OHYDAL.OhyMongoClient client = new OHYDAL.OhyMongoClient();
    HttpCookie co = HttpContext.Current.Request.Cookies.Get("User");
    if (co != null)
    {
        ObjectId id = ObjectId.Parse(co.Value);
        userlogin = client.FindOne<UM>(new { _id = id });
    }
    
}
